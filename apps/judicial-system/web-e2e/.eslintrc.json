{
  "extends": ["../../../.eslintrc.json", "plugin:cypress/recommended"],
  "rules": {
    "no-prototype-builtins": "off",
    "@typescript-eslint/triple-slash-reference": "off"
  },
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "parserOptions": {
